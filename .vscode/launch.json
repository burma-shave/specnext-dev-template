{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "dezog",
            "request": "launch",
            "name": "CSpect",
            "remoteType": "cspect",
            "sjasmplus": [
                {
                    "path": "./out/main.sld"
                }
            ],
            "history": {
                "reverseDebugInstructionCount": 1000000,
                "spotCount": 10
            },
            "startAutomatically": false,
            "commandsAfterLaunch": [
                "-logpoint enable",
                "-assertion enable"
            ],
            "disassemblerArgs": {
                "esxdosRst": true
            },
            "rootFolder": "${workspaceFolder}",
            "topOfStack": "stack_top",
            //"load": "z80-sample-program.sna",
            "load": "./out/main.nex",
            "smallValuesMaximum": 513,
            "tmpDir": ".tmp"
        }
    ]
}